# My First python code
print('hello world)

# Greater or Lesser
num = input('Enter a num between 1 to 10')
if num < 5:
  print(f'{num} is LESS than 5')
elif num > 5:
  print(f'{num} is GREATER than 5')
else:
  print(f'{num} is EQUAL to 5')

# Prime Number logic
num =  input('Enter a number between 1 to 100: ')

prime = False
for i in range(2,num):
  if num % i == 0:
      prime = True

if prime:
  print(f'{num} is a PRIME')
else:
  print(f'{num} is NOT a PRIME')
      
